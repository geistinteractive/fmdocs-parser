{
  "name": "fmdocs-parser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "alias": "now alias",
    "scale": "now scale fmdocs-parser.geist.sh 1 3",
    "predeploy": "npm run build-ts && now switch geistinteractive",
    "deploy:production": "npm run predeploy && now -e NODE_ENV=\"production\" && npm run alias && npm run scale"
  },
  "author": "todd@geistinteractive.com",
  "license": "MIT",
  "dependencies": {
    "@types/dotenv": "^4.0.2",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-json-error-handler": "^1.0.0",
    "fast-xml-parser": "^3.2.2",
    "http-errors": "^1.6.2",
    "npmlog": "^4.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/node": "^9.4.6",
    "concurrently": "^3.5.1",
    "typescript": "^2.7.2"
  }
}
